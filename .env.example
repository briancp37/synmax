# SynMax Data Agent Environment Variables
# Copy this file to .env and fill in your API keys

# OpenAI API Key (for gpt-4.1, gpt-5 models)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (for claude-sonnet, claude-opus models)  
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Default model to use
DATA_AGENT_MODEL=gpt-5

# Cache settings
CACHE_TTL_HOURS=24
CACHE_MAX_GB=5
ROW_CKPT=100000
BYTE_CKPT=100000000

# Data path (will be set automatically if using auto-download)
DATA_PATH=data/pipeline_data.parquet
